<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mustofa.shipinspector" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".Login"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".BeforeMainActivity"
            android:theme="@style/Theme.AppCompat.Dialog" >
        </activity>
        <activity android:name=".MaterialForm"
            android:label="Material Identification">
        </activity>
        <activity android:name=".Help"
            android:label="Help">
        </activity>
        <activity android:name=".PrepFabForm" >
        </activity>
        <activity android:name=".SubAssemblyForm"
            android:label="Sub Assembly">
        </activity>
        <activity android:name=".AssemblyForm"
            android:label="Assembly">
        </activity>
        <activity android:name=".GrandAssemblyForm"
            android:label="Grand Assembly">
        </activity>
        <activity android:name=".ErectionForm"
            android:label="Erection">
        </activity>
    </application>

</manifest>
